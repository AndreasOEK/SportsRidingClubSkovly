@page "/browseSessions"
@rendermode InteractiveServer

<PageTitle>Browse Sessions</PageTitle>
<h3>Browse Session</h3>

<p>Input Session Description Here</p>
<input @oninput="InputChanged" />
<p> Hello!
    @message
</p>
@code {
    string message = "Yes";

    void InputChanged(ChangeEventArgs e)
    {
        message = (string)e.Value;
    }
}
